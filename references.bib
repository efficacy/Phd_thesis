@article{Kupp2012,
	title = {Applying the model-view-controller paradigm to adaptive test},
	volume = {29},
	issn = {07407475},
	doi = {10.1109/MDT.2011.2179370},
	abstract = {The semiconductor industry is continually moving towards higher levels of integration and ever-more complex devices, making yield learning and process improvement commensurately challenging. One particularly challenging domain of semiconductor manufacturing is testing, an increasingly expensive endeavor due to the costly automated test equipment required. In response, industrial groups are actively developing means of dynamically modifying test programs to more efficiently test hardware, via an approach known as adaptive test. The deployment of such adaptive test systems can be quite challenging due to the many requirements placed on the system, including the ability to rapidly process data from millions of devices, ease of integrating novel adaptive test techniques, and ability to permit test engineers to easily monitor the system. These test challenges hint at broader data stratification challenges facing semiconductor manufacturing. In this work we motivate the need for bringing modern compute methods to bear on semiconductor manufacturing, and particularly adaptive test, via the software architecture known as model-view-controller, a pattern that is already widely used in software engineering.},
	number = {1},
