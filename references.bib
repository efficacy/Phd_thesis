	author = {Fraile, Juan and Panadero, Ernesto and Pardo, Rodrigo},
	year = {2017},
	keywords = {Assessment, Co-creation, Q, Rubric, Self-assessment, Self-efficacy, Self-regulation, Teaching},
}

