@article{Stern1993,
	title = {Authenticity and the textual persona: postmodern paradoxes in advertising narrative},
	abstract = {This paper discusses the concept of authenticity in adver-tising in terms of the complementarity of paradoxes that relate to the persona. The focus is on advertising text, for these issues are treated in reference to the management of the persona's paradoxical components to achieve authenticity. The paper begins by defining authenticity in advertismg and goes on to treat the persona as the sponsor's representative in narrated messages. This discussion links advertising with its literary antecedents in order to illustrate the relationship between authenticity and the within-ad persona. The paper next considers the persona's paradoxical components -real-ity/fictionality, originality/reproducibility, and revelation/ concealment. The postmodern theory of complementarity is proposed as a means for understanding the coexistence of these paradoxical opposites. Last, the paper suggests areas for future research on consumer perceptions of authenticity, global advertising and the consumer response known as " sus-pension of disbelief. " The concerted effort of a culture or of a segment of a culture to achieve authenticity generates its own conventions, its generalities, its commonplaces, its maxims, what Sartre, taking the word from Heidegger, calls the " gabble " (Trilling 1972, p. 105). Advertising is the " gabble " of commercial culture -an expression of the postmodern Correspondence to: B. Stern, 160 East 84tb Street -16C, New York, NY 10028, USA. Tel/Fax: (212) 5352610. The author acknowledges the support of a Graduate School-Newark Re-search Award for partial funding of this project.},
	urldate = {2018-03-11},
	author = {Stern, Barbara},
	year = {1993},
	keywords = {Q, Writing},
}

@inproceedings{Denoue2015,
	title = {“ {Show} me the {Action} ”},
	isbn = {978-1-4503-3307-8},
	abstract = {Live meeting documents require different techniques for effectively retrieving important pieces of information. During live meetings, people share web sites, edit presentation slides, and share code editors. A simple approach is to index with Optical Character Recognition (OCR) the video frames, or key-frames, being shared and let user retrieve them. Here we show that a more useful approach is to look at what actions users take inside the live document streams. Based on observations of real meetings, we focus on two important signals: text editing and mouse cursor motion. We describe the detection of text and cursor motion, their implementation in our WebRTC (Web Real-Time Communication)-based system, and how users are better able to search live documents during a meeting based on these extracted actions.},
	author = {Denoue, Laurent and Carter, Scott and Cooper, Matthew},
	year = {2015},
	keywords = {Q, doceng, indexing, live document search, ocr, real-time search, screen-, sharing, video conferencing},
	pages = {195--198},
}

