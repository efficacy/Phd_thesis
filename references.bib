@article{Penzenstadler2020a,
	title = {Requirements engineering for sustainability: an awareness framework for designing software systems for a better tomorrow},
	volume = {25},
	doi = {10.1007/s00766-020-00336-y},
	abstract = {Integrating novel software systems in our society, economy and environment can have far-reaching efects. As a result, software systems should be designed in such a way as to maintain or improve the sustainability of their intended socio-technical systems. However, a paradigm shift is required to raise awareness of software professionals on the potential sustainability efects of software systems. While Requirements Engineering is considered the key for driving this change, requirements engineers lack the knowledge, experience and methodological support for acting as facilitators for a broader discussion on sustainability efects. This paper presents a question-based framework for raising awareness of the potential efects of software systems on sustainability, as the frst step towards enabling the required paradigm shift. An evaluation study of the framework was conducted with four groups of computer science students. The results of the study indicate that the framework is applicable to diferent types of systems and helps to facilitate discussions about the potential efects that software systems could have on sustainability.},
	journal = {Requirements Engineering},
	author = {Penzenstadler, Birgit and Duboc, Letitia Leticia and Porras, Jari and Kocak, Sedef Akinli and Betz, Stefanie and {Chitchyan Ruzanna} and Leifler, Ola and Seyff, Norbert and Venters, Colin C. and Penzenstadler, Birgit and Porras, Jari and Akinli Kocak, Sedef and Betz, Stefanie and Chitchyan, Ruzanna and Leifler, Ola and Seyff, Norbert and Venters, Colin C.},
