	year = {2010},
	keywords = {Components, PhD2020, Q},
	pages = {618--624},
}

@article{Motahhir2019,
	title = {Open hardware/software test bench for solar tracker with virtual instrumentation},
	volume = {31},
	issn = {22131388},
	doi = {10.1016/j.seta.2018.11.003},
	abstract = {This work develops an open hardware/Software Test bench for solar tracker. The proposed prototype is based on a dual axis solar tracker controlled with Arduino Uno which is an open-source prototyping platform based on easy-to-use hardware and software. The solar tracker can be controlled automatically with the help of Light Dependent Resistor (LDR) sensors or manually using a potentiometer. Moreover, this test bench provides a virtual instrumentation based on Excel in which its solar tracker data can be recorded and presented. The hardware used has been chosen to be inexpensive, compact and versatile. The proposed test bench enables largescale solar tracker research, innovative new curriculum, and solar tracker outreach to younger students. A test of all its functionalities is done, the results show that it is ﬂexible, easy to use. For instance, it can be used indoors using a lamp.},
