@incollection{Tirel2025,
	address = {Cham},
	title = {When {Good} {Enough} is the {Best} {Option}: {Use} of {Digital} {Sufficiency} to {Fight} {Climate} {Change}},
	volume = {15342},
	isbn = {978-3-031-78089-9 978-3-031-78090-5},
