	keywords = {PhD2020, Q, Role understanding, Software Engineering, SusAF, Sustainability, Sustainability Design},
	pages = {294--299},
}

@techreport{Swacha2022,
	title = {Models of {Sustainable} {Software}: {A} {Scoping} {Review}},
	url = {https://doi.org/10.3390/su14010551},
	abstract = {Information and Communication Technologies (ICTs) play a double role in the strife for sustainable development goals, as both an enabler of green solutions and a cause of excessive consumption. While the primary focus of sustainability-related research is on the hardware aspect of ICT, its software aspect also deserves attention. In order for the notion of green and sustainable software to become widespread among practitioners, models are needed, both to be used as a reference on how to develop sustainable software, and to check whether given software or its development process is sustainable. In this paper, we present the results of a scoping review of literature on sustainable software models, based on 41 works extracted from an initial set of 178 query results from four bibliographic data providers. The relevant literature is mapped using five categories (model scope, purpose, covered sustainability aspects, verification or validation, and the economic category of the country of research), allowing us to identify recent trends and research gaps, which can be addressed in future work.},
