	journal = {New Generation Computing},
	author = {Asano, Yu and Koide, Seiji and Iwayama, Makoto and Kato, Fumihiro and Kobayashi, Iwao and Mima, Tadashi and Ohmukai, Ikki and Takeda, Hideaki},
