@article{ShSagar2008,
	title = {The {Oea} framework for class-based object-oriented style {JavaScript} for web programming},
	volume = {31},
	doi = {10.1016/j.csi.2008.03.014},
	abstract = {In this paper we reflect on the differences between prototype-based and class-based programming languages and we introduce a new approach, called the Oea framework, that will allow JavaScript developers to write programs using a class-based style, which we assert is easy to use, has a syntax style that resembles that of the Java class-based approach and has high performance and reliability. We present a survey of the most widely used class-based techniques for JavaScript describing their methods and highlighting their shortcomings and compare the new approach with this earlier work. The approach has been developed in the context of a framework for developing Web applications using Scalable Vector Graphics (SVG), JavaScript and the Resource Description Framework (RDF) which involved the construction of substantial libraries and other code using the approach.},
