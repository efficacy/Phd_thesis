	author = {Nazir, Shah and Anwar, Sajid and Khan, S.A. Sher Afzal and Shahzad, Sara and Ali, Muhammad and Amin, Rohul and Nawaz, Muhammad and Lazaridis, Pavlos and Cosmas, John},
