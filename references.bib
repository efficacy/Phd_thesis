@inproceedings{Gipp2011,
	title = {Citation {Pattern} {Matching} {Algorithms} for {Citation}-based {Plagiarism} {Detection}: {Greedy} {Citation} {Tiling}, {Citation} {Chunking} and {Longest} {Common} {Citation} {Sequence}},
	isbn = {978-1-4503-0863-2},
	doi = {10.1145/2034691.2034741},
	abstract = {Plagiarism Detection Systems have been developed to locate instances{\textbackslash}nof plagiarism e.g. within scientific papers. Studies have shown that{\textbackslash}nthe existing approaches deliver reasonable results in identifying{\textbackslash}ncopy\&paste plagiarism, but fail to detect more sophisticated forms{\textbackslash}nsuch as paraphrased, translated or idea plagiarism. The authors of{\textbackslash}nthis paper demonstrated in recent studies [4, 15] that the detection{\textbackslash}nrate can be significantly improved by not only relying on text analysis,{\textbackslash}nbut by additionally analyzing the citations of a document. Citations{\textbackslash}nare valuable language independent markers that are similar to a fingerprint.{\textbackslash}nIn fact, our examinations of real world cases have shown that the{\textbackslash}norder of citations in a document often remains similar even if the{\textbackslash}ntext has been strongly paraphrased or translated in order to disguise{\textbackslash}nplagiarism. This paper introduces three algorithms and discusses{\textbackslash}ntheir suitability for the purpose of Citation-based Plagiarism Detection.{\textbackslash}nDue to the numerous ways in which plagiarism can occur, these algorithms{\textbackslash}nneed to be versatile. They must be capable of detecting transpositions,{\textbackslash}nscaling and combinations in a local and global form. The algorithms{\textbackslash}nare coined Greedy Citation Tiling, Citation Chunking and Longest{\textbackslash}nCommon Citation Sequence. The evaluation showed that common forms{\textbackslash}nof plagiarism can be detected reliably if these algorithms are combined.},
	booktitle = {Proceedings of the 11th {ACM} {Symposium} on {Document} engineering - {DocEng} '11},
	author = {Gipp, Bela and Meuschke, Norman},
	year = {2011},
	keywords = {Assessment, Q, Research, citation order analysis, citation pattern analysis, citation-based, doceng, plagiarism detection systems},
