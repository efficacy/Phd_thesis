@article{Shokunbi2022,
	title = {Inducing {Software} {Sustainavbility}},
	volume = {1},
	abstract = {Modern societies rely heavily on complex, large-scale, software-intensive systems that are increasingly operating in a continuously available environment, making it hard to adapt to changing stakeholder goals and expectations. Any software system's foundation is its architecture, which provides a mechanism for reasoning about key software quality needs. Their long-term viability —the ability to adapt to changing circumstances – is a major problem in software architecture study and practice. As part of the entire software design and development process, accidental software complexity accumulates progressively over time. This allows several issues to overlap from a software architecture perspective, including, but not limited to: the accumulation of technical debt design decisions of individual components and systems leading to coupling and cohesion issues; the application of tacit architectural knowledge resulting in unsystematic and undocumented design decisions; architectural knowledge vaporization of design choices and the organization's continued ability to understand the aforementioned as to accomplish efficient and effective maintenance and evolutionary change, sustainable software architectures must evolve over the whole lifecycle of the system, from basic design genesis to end-of-life. To give a context and terminology for structuring the discourse on software architectures and sustainability, this article provides general ideas and perspectives on sustainability in relation to software systems. It summarizes some of the recent research trends and techniques with regards to explicitly addressing sustainability in the context of software architectures, with a focus on the capability of software architectures and architectural design decisions to endure through time. The primary goal of this article is to give a foundation and roadmap for future research themes in the field of sustainable software architectures, covering recent trends as well as unresolved issues and research problems.},
	number = {1},
	author = {Shokunbi, Opeyemi O and Maitanmi, Stephen O},
	year = {2022},
	keywords = {PhD2020, Q},
}

@inproceedings{Bieser2018,
	title = {Indirect {Effects} of the {Digital} {Transformation} on {Environmental} {Sustainability}: {Methodological} {Challenges} in {Assessing} the {Greenhouse} {Gas} {Abatement} {Potential} of {ICT}},
	shorttitle = {Indirect {Effects} of the {Digital} {Transformation} on {Environmental} {Sustainability}},
	doi = {10.29007/lx7q},
	abstract = {The digital transformation has direct and indirect effects on greenhouse gas (GHG) emissions. Direct effects are caused by the production, use and disposal of information and communication technology (ICT) hardware. Indirect effects include the changes to patterns of production and consumption in other domains. Studies quantifying both effects often conclude that net effects (indirect minus direct effects) can lead to a significant GHG emission reduction. We revisited a study by Accenture on ICT’s GHG abatement potential in Switzerland by reassessing the main assumptions. Our results confirm that ICT has the potential to reduce GHG emissions in Switzerland, especially in the building, transport and energy domains. However, our results also suggest that the potential is smaller than anticipated and that exploiting this potential requires targeted action. Reasons for differences among these results (and the results of similar other studies) are: degrees of freedom in the assessment methodology, selection of ICT use cases, allocation of impacts to ICT, definition of the baseline, estimation of the environmental impact, prediction of the future adoption of use cases, estimation of rebound effects, interaction among use cases, and extrapolation from use case to society-wide impacts. We suggest addressing these methodological challenges to improve comparability of results.},
	urldate = {2023-11-23},
	booktitle = {{ICT4S2018}. 5th {International} {Conference} on {Information} and {Communication} {Technology} for {Sustainability}},
	author = {Bieser, Jan C. T. and Hilty, Lorenz M.},
	year = {2018},
	keywords = {PhD2020, Q},
	pages = {68--53},
}

