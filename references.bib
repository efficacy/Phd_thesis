	volume = {61},
	doi = {10.1016/j.compedu.2012.09.011},
	abstract = {a b s t r a c t Lecture recordings are increasingly used to supplement lecture attendance within higher education, but their impact on student learning remains unclear. Here we describe a study to evaluate student use of lecture recordings and quantify their impact on academic performance. Questionnaire responses and online monitoring of student's access to recordings indicate that w75\% students use this material, the majority in a targeted manner. In contrast, a small subset of students (w5\%) are highly dependent on recordings downloading every lecture, and viewing the material for long periods, such that this repre-sents a large proportion of their independent study. This 'high user' group is atypical, as it contains a high proportion of dyslexic and Non-English Speaking Background students. Despite high usage, lecture recordings do not have a significant impact on academic performance, either across the cohort or with students that use the recordings. Overall, this approach appears to be beneficial, but may reduce lecture attendance and encourage surface learning approaches in a minority of students.},
	urldate = {2018-05-24},
	journal = {Computers \& Education},
	author = {Leadbeater, Wendy and Shuttleworth, Tom and Couperthwaite, John and Nightingale, Karl P},
	year = {2013},
	keywords = {Action reasearch, Dyslexia, Evaluation of impact, Lecture recording, Q, Teaching},
	pages = {185--192},
}

@phdthesis{Le2012,
	title = {A {Transformation}-{Based} {Model} {Integration} {Framework} to {Support} {Iteration} {Management} in {Engineering} {Design}},
	abstract = {In today’s globalised and fast-paced world it can be important for manufacturing companies of high tech products to introduce innovations ahead of their competitors. This pressure is put on the development and design process of products too. Effective project planning and management of engineering design processes is a key to timely product delivery. One way to support project planning and management is process modelling. Many process modelling and simulation approaches can be found in engineering design and management literature. However, these approaches are found to be insufficient for modelling iteration-related aspects in engineering design processes: they are not capable of capturing all relevant iteration-related characteristics, i.e., its causes and effects as well as their dynamic relationships, and, at the same time, addressing planning issues related to process structure and project environment in iteration management. Hence, existing modelling approaches may not be able to support iteration management effectively. A Transformation-Based Model Integration Framework was developed to alleviate some of these limitations. This framework employs two popular modelling approaches: activity-based and System Dynamics modelling. The model integration method of this framework was designed to be applicable to already-built activity- based models in order to enable the analysis of process structural details captured in activity-based models in a simulation modelling environment where process dynamics can be added. The developed framework can explicitly capture a wide range of causes and effects of iteration, which in turn can facilitate explanatory and exploratory simulation analyses to investigate iteration behaviour and address planning issues related to both process structure and project environment.},
	school = {Darwin College},
	author = {Le, Hoang Nam},
	year = {2012},
	note = {Issue: June},
	keywords = {NC, PhD, Q},
}

