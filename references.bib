@phdthesis{Huhtanen2024,
	title = {{ENVIRONMENTAL} {SUSTAINABILITY} {IN} {SOFTWARE} {DEVELOPMENT} {PROCESS}},
	abstract = {The research was conducted through a systematic literature review. The review protocol included two evaluation rounds and was designed to find the most relevant sources regarding environmental sustainability and software development process. The final study selection included 32 articles from which a total of 108 sustainability factors were identified and categorized into 14 recurring themes. The literature review show that sustainability factors in current literature span throughout all life cycle phases, from planning to retirement. The earlier life cycle phases are more closely examined in terms of number and influence of the sustainability factors. The sustainability factors include both process-level guidelines as well as code-level practices, well demonstrated by the two most prominent themes: sustainable project management and code-level characteristics. Key finding of this thesis is that environmental sustainability practices should be integrated into the software development process as an additional layer, rather than as entirely new concepts. Integrating sustainability into the development process includes considering sustainability a key requirement and a quality attribute, as well as adding sustainability assessment into the project management and introducing a sustainability stakeholder into software projects. Identifying sustainability factors and their relevance to life cycle phases helps create awareness of existing measures that can be taken to embed environmental sustainability into software development process. However, further empiric research is required to provide further information about the effectiveness of various sustainability practices.},
	language = {en},
	author = {Huhtanen, Essi},
	year = {2024},
	keywords = {PhD2020, Q},
}

@article{Kandimalla2024,
	title = {Green {Coding} {Practices} in {Software} {Development}: {Perceptions}, {Barriers}, and {Strategies} for {Change}},
	abstract = {Background: The rapid growth of the Information and Communication Technology (ICT) sector has led to significant increases in energy consumption, raising concerns about the environmental impact of software development. Green coding practices have emerged as a potential solution, aiming to reduce the carbon footprint of software by optimizing code for energy efficiency.
Objectives: This study explores how green coding practices are perceived and implemented in the software development industry, identifies the barriers developers face in adopting these practices, and proposes strategies to overcome these challenges.
Methods: A qualitative, case study approach was employed, involving semi-structured interviews with developers from eight companies across various sectors. Thematic analysis was used to identify recurring themes and barriers, and NVivo software facilitated the data coding and analysis process.
Results: The findings reveal that while developers often engage in practices aligned with green coding, there is a widespread lack of awareness and formal organizational support. Key barriers include the absence of formal guidelines, lack of awareness, and a lack of energy monitoring tools. Despite these challenges, developers emphasize the importance of education, leadership advocacy, and the standardization of green coding practices to promote sustainability.
Conclusions: The study highlights the need for industry-wide education, formal guidelines, and the development of energy monitoring tools to facilitate the adoption of green coding practices. By addressing these barriers, companies can significantly reduce their environmental impact, contributing to broader sustainability goals.},
	language = {en},
	author = {Kandimalla, Amuktha Malyada and Bolla, Abhinav},
	year = {2024},
	keywords = {PhD2020, Q},
}

@article{Bamiduro2024,
	title = {Challenges in {Incorporating} {Sustainability} {Practices} in the {Software} {Lifecycle}⋆},
	abstract = {As software becomes more integral to daily life and business operations, its sustainability impact is increasing, and increasingly recognized. This study investigates the key challenges hindering stakeholders from adopting and implementing sustainability practices within organizations. To this aim, we interview 15 practitioners in two organizations to identify the challenges in sustainability adoption and implementation of sustainability practices in the daily workflow of software professionals. Our results highlight the practical challenges, recommendations, and benefits of implementing sustainability practices in real-world organizational settings. Lack of sustainability prioritization, knowledge awareness, measurement tools, scarce resources, and organizational culture are the key challenge areas. By balancing priorities, the investment in sustainability education and tool support coupled with cultural change and resource-rich incentivization programs can motivate practitioners to use sustainability practices. If so, organizations could save money and energy, improve overall productivity, and foster personal satisfaction.},
	language = {en},
	journal = {Software Business},
	author = {Bamiduro, Mercy and Fatima, Iffat and Lago, Patricia and Vos, Sophie},
	year = {2024},
	keywords = {PhD2020, Q},
}

@article{Anjana2025,
	title = {Energize sustainability: {EnSAF} for sustainability aware, software intensive energy management systems},
	volume = {178},
	issn = {09505849},
	shorttitle = {Energize sustainability},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S095058492400212X},
	doi = {10.1016/j.infsof.2024.107607},
	abstract = {Objective: Designing a software-intensive sustainable energy management system requires considering technical, environmental, social, and economic factors. This helps evaluate an EnMS’s overall impact and improve its design. We proposed EnSAF for efficient utilization of the energy incurred for the design of sustainability-aware EnMSs.
Method: In this work, EnMSs in diverse use cases were selected and analyzed in terms of technical, social, environmental, and economic dimensions of sustainability in collaboration with various stakeholders. The set of application-specific design concerns and Quality Attributes (QAs) were addressed by the Sustainability Assessment Framework (SAF) toolkit. The resultant SAF instances of each EnMS, derived through the analysis and discussion with the stakeholders, were then analyzed to advocate the DMs and SQ model for generic EnMSs.
Results: This study demonstrated the following outcomes (i) technical concerns dominate the existing EnMSs (ii) integration of renewable energy resources reduces dependency to the main power grid and nurtures a sustainable environment by diminishing carbon footprint, and minimizing payback time, in the economic dimension; (iii) extant definitions of quality attributes need significant scrutiny and updates apropos of objectives of EnMSs
Conclusion: The SAF toolkit was found to be deficient in the representation of relevant design concerns and quality attributes concomitant with sustainable EnMS. Prevailing DMs are inept to factor in stakeholder’s concerns, as the model is ill-equipped to account for spatio-temporal representation of QAs. Pursuant to the insights from the 4 SAF instances, a generic framework, EnSAF, is proposed to tackle the relevant concerns apropos of EnMS sustainability. This work proposed a representation of DMs in the SAF toolkit specifically for sustainability-aware EnMS.},
	language = {en},
	urldate = {2024-11-24},
	journal = {Information and Software Technology},
	author = {Anjana, M.S and Lago, Patricia and Devidas, Aryadevi Remanidevi and Ramesh, Maneesha Vinodini},
	month = feb,
	year = {2025},
	keywords = {PhD2020, Q},
	pages = {107607},
}

@misc{Wasif2024,
	title = {Green {Software} {Engineering} as a {Catalyst} for {Renewable} {Energy} {Efficiency} and {Sustainable} {Development}},
	url = {https://rgdoi.net/10.13140/RG.2.2.30533.05601},
	doi = {10.13140/RG.2.2.30533.05601},
	abstract = {Green Software Engineering (GSE) is emerging as a key enabler in improving the efficiency of renewable energy systems and fostering sustainable development. As the global shift towards clean energy accelerates, the role of software in optimizing energy systems has become increasingly significant. GSE, a discipline focused on minimizing the environmental impact of software development and operation, can act as a catalyst for enhancing the performance of renewable energy technologies such as solar, wind, and hydropower. By optimizing algorithms, improving data management, and reducing the computational footprint, green software can drive down energy consumption in smart grids, data centers, and Internet of Things (IoT) devices, making renewable energy systems more efficient. Moreover, GSE supports the adoption of low-power programming techniques and sustainable cloud computing, which are essential in managing large-scale renewable energy infrastructures. The integration of machine learning and artificial intelligence into GSE can further boost predictive maintenance, resource allocation, and energy distribution, thus reducing waste and enhancing the stability of renewable energy networks. In addition, GSE principles align with the United Nations Sustainable Development Goals (SDGs), particularly those related to affordable clean energy, responsible consumption, and climate action. This paper explores the synergies between Green Software Engineering and renewable energy, demonstrating how GSE practices contribute to reducing carbon footprints, enhancing energy efficiency, and driving sustainable development. It also highlights the importance of interdisciplinary collaboration between software engineers, energy experts, and policymakers to create a future where technology not only supports economic growth but also minimizes environmental impact.},
	language = {en},
	urldate = {2024-11-24},
	publisher = {Unpublished},
	author = {Wasif, Nomi},
	year = {2024},
	keywords = {PhD2020, Q},
}

