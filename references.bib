}

@article{Garcia-Izquierdo2012,
	title = {Is the browser the side for templating?},
	volume = {16},
	issn = {10897801},
	doi = {10.1109/MIC.2011.81},
	abstract = {Browser-side templating (BST) is a valid alternative for Web development, even when it comes to building accessible applications. BST processes templates in the browser using a JavaScript-coded engine, providing significant performance improvements and making model-view separation a reality. However, it also has significant drawbacks. BST's dependence on JavaScript affects its accessibility and hides the delivered pages' content from search engines, hampering Web visibility. The authors confront this dilemma and propose a technique that lets BST be accessible and semantically crawlable, while preserving its advantages. © 2006 IEEE.},
