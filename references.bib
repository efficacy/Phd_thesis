@inproceedings{Fernandez1998,
	title = {Catching the boat with \{{Strudel}\}: experiences with a web-site management system},
	isbn = {0-89791-955-5},
	abstract = {The Strudel system applies concepts from database{\textbackslash}nmanagement systems to the process of building Web{\textbackslash}nsites. The key idea is separating the {\textbackslash}nmanagement of the site's data, the creation and management of the{\textbackslash}nsite's structure, and the graphical presentation of the site's pages.{\textbackslash}nFirst, the site builder creates a uniform model of all the data{\textbackslash}navailable at the site. Second, the site builder uses this model to{\textbackslash}ndeclaratively define the Web site's structure by applying a{\textbackslash}n``site-definition query'' to the underlying data. The result of{\textbackslash}nevaluating this query is a ``site graph'', which represents both the{\textbackslash}nsite's content and structure. Third, the site builder specifies the{\textbackslash}ngraphical presentation of pages in Strudel's HTML-template language.{\textbackslash}nThe data model underlying Strudel is a semi-structured model of labeled{\textbackslash}ndirected graphs.},
