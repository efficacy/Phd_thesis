@article{Barnett1985,
	title = {{THE} {PRIMITIVE} {STRING} {MANIPULATION} ({PRISM}) {LANGUAGE}},
	volume = {20},
	abstract = {This note describes a set of primitives that seem to provide th e basis of a general system of string manipulation . The author i s using these in some simulation experiments and in instructiona l material that he is developing . He would appreciate citations t o any prior work that uses the same set of primitives, or which ha s strong similarities to the PRISM approach . Individual details o f PRISM have obvious resemblances to particular features of som e well known programming languages and formal systems . The precis e combination of primitives in PRISM, however, have capabilitie s that do not seem to be known generally, including possible use i n the design of artificial intelligence machines . 1 . THE LANGUAGE CONSTRUCT S The PRISM language pertains to a machine that consists of (1) a n input station, (2) a workspace in which a character string can b e loaded and altered by certain operations, (3) a command space i n which a character string that expresses a series of commands ca n be loaded and executed, (-4) an output station, and (5) a backing store . In the interactive simulation that we have programmed fo r the IBM PC, the input and output stations are the keyboard an d the screen, respectively, and the backing store is the disk . Our simulated PRISM machine begins operation by prompting for a n external command : (L)oad, (S)ave, (E)xecute, (P)rint or (Q)uit :},
	urldate = {2017-04-10},
	journal = {SIGPLAN Notices},
	author = {Barnett, Michael P},
	year = {1985},
	keywords = {PhD, Q},
}

@article{Barnett1985a,
	title = {A {SIMPLE} {MECHANISM} {THAT} {SUPPORTS} {THE} {PRISM} {LANGUAGE}},
	volume = {20},
	abstract = {The PRISM language that we described recently consists of a small set of string manipulating primitives [I]. It was specified as a step towards postulating simple mechanisms to manipulate strings. This present working paper discusses a mechanism that we call a Primitive String Processor (PSP). It has some simple features, based on associative memory principles, that support a major part of the PRISM language easily. Since defining PRISM, we found that deletion and insertion can be performed by using replacement and execute string commands. As a result, the PSP can perform an interesting variety of processes using a very small operation set. These are discussed, together with the further provisions needed in a general string manipulating system. The detailed examples were checked by a simple program that simulates a PSP.},
	urldate = {2017-04-10},
	journal = {SIGPLAN Notices},
	author = {Barnett, Michael P},
	year = {1985},
	keywords = {PhD, Q},
}

@article{Barnett1963,
	title = {Continued {Operation} {Notation} for {Symbol} {Manipulation} and {Array} {Processing}},
	volume = {6},
	abstract = {There is bound to be a certain amount of trouble as the new compilers come into use and existing programs have to be altered. To help deal with this problem, the SNARE FORTRAN Committee have produced a program, SIFT, for the I B M 7090, which will automatically convert a source routine from Fortran I I to Fortran IV. Also, the U.K.A.E.A. (A.E.E., Winfrith) have written a program, CONFLAG, for the I B M 1401, which scans a routine and points out the places where alteration may be needed. In general, however, we expect that relatively few rou-tines would need to be changed, and the places needing attention would be easy to find--at the head of the routine, or with a letter in column 1. We have concentrated in this report on the Fortran source language, and excluded discussion of the operating systems (e.g. Fortran Monitor System). There is a logical distinction between these two, and we think it desirable not to obscure it. One of the reasons is that the operating system is considerably more machine dependent, as it must allow explicit reference to the particular input/ output equipment of the computer. The various systems concerned are similar in outline, providing compilation or assembly of independent routines, and optional loading and execution. However, there is so much variety of detail that it would not be appropriate to discuss them here.},
	number = {8},
	urldate = {2017-04-10},
	journal = {Communications of the ACM},
	author = {Barnett, Michael P},
	year = {1963},
	keywords = {PhD, Q},
}

