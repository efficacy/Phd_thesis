@inproceedings{Balinsky2011a,
	title = {Secure {Document} {Engineering}},
	isbn = {978-1-4503-0863-2},
	doi = {10.1145/2034691.2034746},
	abstract = {With the boom in interactive and composite documents and the increased{\textbackslash}ncoupling between the on-line and physical worlds, the need for secure{\textbackslash}ndocument engineering is greater than ever. Four important factors{\textbackslash}ncontribute to the need to re-engineer document lifecycles and the{\textbackslash}nassociated workflows. The first is the rapid increase in mobile access{\textbackslash}nto documents. The second is the movement of documents from private{\textbackslash}ndirectories, shared directories and intranets to the cloud. The third is{\textbackslash}nthe increased generation of - and expectation for - document content,{\textbackslash}ncontext and use analytics. Finally, the proliferation of social website{\textbackslash}napplications and services over the past half-decade have created for{\textbackslash}nmany a constant state of log-in. Each of these trends creates a{\textbackslash}nsignificantly increased ``attack surface\{''\} for individuals,{\textbackslash}norganizations and governments interested in breaching the privacy and{\textbackslash}nsecurity of web users. Combined, these transformations create as big a{\textbackslash}nchange to content security as that of the browser in the 1990s. In this{\textbackslash}nworkshop, we consider the impact of these ongoing transformations in{\textbackslash}ndocument creation and interaction, and consider what the best approaches{\textbackslash}nwill be to provide privacy and security in light of these{\textbackslash}ntransformations.},
	booktitle = {Doceng 2011: {Proceedings} of the 2011 {Acm} {Symposium} on {Document} {Engineering}},
	author = {Balinsky, Helen and Simske, Steven},
	year = {2011},
	note = {Issue: 2},
	keywords = {Q, Security; Publicly Posted Composite Document; Acce, doceng},
	pages = {269--272},
}

