@article{Barnet1990,
	title = {{PRIMITIVE} {STRING} {TRANSFORMATIONS} {AS} {REDUCTIONS} {TO} {NORMAL} {FOR} {M}},
	volume = {25},
	abstract = {The action of a string transformation language, which contains a very small number of primi -tives and was suggested by a sub-cellular cognitive model, is presented as a reduction to normal form . The inductive proof of algorithms that are expressed in this language is illustrated by a simple example . 1. INTRODUCTIO N This note updates an earlier working paper [1] on an approach to symbol manipulatio n that involved an abstract computing device and a set of constructs, that we called a Primitiv e String Processor (PSP) and the PRISM language, respectively . The work was started in an effort to define information processing mechanisms that might have parallels in biological sys-tems . This topic has been expanded in [2], which also relates the proposed mechanisms to th e developing field of molecular electronic devices (MEDs) . These are chemical and solid stat e systems that are being projected as computer components of molecular dimensions [3, 4] . Because of this further work, an improvement of the earlier notation is reported here, fo r reference, and related to the ideas of applicative programming and reduction to normal form . Algorithms that are expressed in the improved notation are amenable to formal proofs that, i n turn, seem suited to mechanization . We now use the term Primitive String Transformer, abbre-viated to PST, for the abstract device and the language that it supports (the abbreviation PS P had a prior use in biology) . A program to interpret PST algorithms has been written in FOR-TRAN, for simulation experiments on parallel processing systems [5] .},
	number = {5},
	urldate = {2017-04-10},
	journal = {SIGPLAN Notices},
	author = {Barnet, Michael P},
	year = {1990},
	keywords = {PhD, Q},
}

@article{Barnes2007,
	title = {Teaching and {Learning} with the {Net} {Generation}},
	volume = {3},
	abstract = {A decade ago, the first wave of the Net Generation began to enter college, forcing educational institutions to deal with a new population of learners with unique characteristics. With the Net Generation representing nearly 7\% of the population today (Bartlett 2005) and with nearly 49.5 million students enrolled in schools in 2003 (Enrollment Management Report 2005), responding to the specific needs of this generation of learners is becoming increasingly important. The challenge of evolving pedagogy to meet the needs of Net-savvy students is daunting, but educators are assisted by the fact that this generation values education. These students learn in a different way than their predecessors did, but they do want to learn. In this article we will define the characteristics of Net Geners' learning styles and discuss how educators can make the most of these particular traits.},
