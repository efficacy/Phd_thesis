@phdthesis{Bianchi2006,
	title = {Dynamic {Localization} of {Components} in {Distributed} {Systems}},
	abstract = {The widespread diffusion of the Internet, started in the last decade of the 20th century, has caused a renewed attemption to the research activities on distributed environments. In fact, the interest of the scientific and industrial community in this last years has been oriented towards problems of the resource sharing either hardware and software. In this context, one important issue concerns the localization of resources. For this reason, in this thesis I deal with some problems affecting the discovery systems of four important different areas of distributed computing: Web services, RMI Java technology, distributed Java virtual machines and computational Grid. More precisely, I propose an enhanced UDDI registry providing predictions about the run-time behavior of Web services; its adoption can improve the phase of the dynamic locate and selection of service instances. With respect to the Java RMI technology, I propose the extension of the naming service provided by the RMI registry with a directory service. The proposed framework makes it possible the de- velopment of reliable, performant, and high-available RMI applications. Furthermore, a second framework, able to generate gateway RMI/Web services, is presented. This framework permits a transparent integration of RPC-Web services in RMI based applications. Then, I report on a recently started research activity focusing on the Java thread migration among distributed Java Virtual Machines. I present an implementation of DJVM based on peer-to-peer primitives, able to build large and dynamic JVM farms. In addition, I introduce an ongoing formalization process, which final goal is to extended the Java computational model adding aspects concerning the thread migration issue. Finally, I propose to extend the current Grid Information System (GIS) architecture, with the aim to enhance the discovery, allocation and management of the Grid resources using inferential techniques.},
	school = {University of L'Aquila},
	author = {Bianchi, Marco},
	year = {2006},
	note = {Publication Title: Informatica},
	keywords = {PhD, Q},
}

@inproceedings{Bodhuin2002,
